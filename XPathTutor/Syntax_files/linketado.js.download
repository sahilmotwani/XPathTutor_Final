	}
	
	if (szerepel == 0){
		var randver = getRandomBetween(0, linkOpciok.length-1);	//a es b kozott, inclusive
		//console.log(randver);
		sourceEditorFricc.setValue(sourceEditorFricc.getValue() + linkOpciok[randver]);
